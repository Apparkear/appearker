/************************* Booking ***********************/

.description-holdr.bookings h3 {font-size: 18px; font-weight: 600; padding-bottom: 10px; border-bottom: 1px solid #e8e8e8;}
.book_in {font-size:14px!important; color:#888888;}
.book_out {font-size:14px!important; color:#888888;}
.book_in_date {font-size:16px; color:#282828; font-weight: 500!important;}
.book_out_date {font-size:16px; color:#282828; font-weight: 500!important;}
