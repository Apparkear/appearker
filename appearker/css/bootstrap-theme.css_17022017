.prod-listing li .product-image{width:100%; height: 152px; position: relative;}
.prod-listing li .product-image img{width:100%; height: 100%; position: relative;}
.prod-listing li .product-image .rent-price{position: absolute; left: 0; bottom:15px; background: #ff4d55; padding: 6px 8px; text-align: center; color:#fff}
.prod-listing li .product-image .rent-price b{display: block; line-height: 14px}
.prod-listing li .product-image .rent-price span{font-size:12px; text-transform: uppercase; line-height: 11px}
.prod-listing li .product-info{padding: 10px; background: #fff}
.prod-listing li .product-info h4{color:#ff4d55; margin-top: 0; font-size: 14px; text-transform: uppercase; text-overflow: ellipsis; overflow: hidden; white-space: nowrap; font-weight: 700}
.prod-listing li .icon-holder{display: inline-block; margin-right: 15px; margin-bottom: 8px; color:#505050}
.prod-listing li .icon-holder span {display: inline-block;line-height: 16px;vertical-align: top; margin-right: 5px}

.layout-search .search-map {-moz-box-flex: 1;flex: 1 1 auto; overflow: hidden; position: relative;}

.menu_toggle::before {content: "✕";}
.menu_toggle {-moz-user-select: none;color: #fff;cursor: pointer;float: right;text-align: right; margin-right: 24px;position: relative;top: -9px; display: none;}



/************************* Details ***********************/
a{color:#ff4d55; outline: none}
a:hover{color:#ff2f39; outline: none}

.l-main-menu {height: 100vh;position: fixed;width: 130px; background: #333332}
.l-main-section {background: #f7f5f5;box-sizing: border-box;padding-left: 130px;width: 100%;}
.l-property-body {background: #f7f5f5;padding: 3.52941rem;}
.l-property-body h1{font-size:34px; font-weight:300; margin-top: 0}
.l-property-body .breadcrumb {color: #a5a5a5;font-size: 16px;line-height: 20px;margin: 0; padding: 0; margin-bottom: 10px}
.l-property-body .breadcrumb .loc-icon{font-size: 18px}
.l-property-body .breadcrumb a{display: inline-block; position: relative; padding: 5px 10px; color:#919191}
.l-property-body .breadcrumb a::after{position: absolute; right:-6px; font-family: 'FontAwesome'; content: '\f105'; color:#b2b2b2}
.l-property-body .breadcrumb a:last-child::after{ content: '';}
.custom-tabs {margin-bottom: 20px}
.custom-tabs li a{color: #949494; font-size:16px;}
.custom-tabs li.active a, .custom-tabs li a:hover, .custom-tabs li.active a:focus, .custom-tabs li.active a:hover{color: #ff4d55; border:0; background:none; border-bottom:1px solid #ff4d55}

.menu{padding: 15px 0;}
.tab-pane ul li{height: 400px}
.tab-pane .bx-wrapper .bx-controls-direction a {height: 58px;width: 58px;}
.tab-pane .bx-wrapper .bx-prev{background: url('../images/prev-arrow.png') no-repeat 0 0;}
.tab-pane .bx-wrapper .bx-next{background: url('../images/next-arrow.png') no-repeat 0 0;}
.tab-pane .bx-wrapper .bx-next:hover{background-position: 0 0;}

.details-section{padding-top: 25px; padding-bottom: 25px}
.room-description h3{margin-top: 0}
.room-description p{font-size:16px; font-weight: 300}
.room-description .prod-listing{margin-left: 0; margin-right: 0}
.room-description .prod-listing li{margin:0; width: auto}


.price-box{border:1px solid #e1e1e1; background: #fff; border-radius: 6px; overflow: hidden;}
.price-box .top-area{border-bottom:1px solid #e1e1e1; padding:15px; overflow: hidden; font-size:18px}
.price-box .top-area .price{float: left; color: #6e6e6e}
.price-box .top-area .month{float: right; color: #121212}
.booknow-card-body{padding:15px 15px 5px 15px; border-bottom:1px solid #e1e1e1;}
.booknow-card-body .form-datepicker_container{line-height: 44px}
.price-box-footer{padding:15px;}
.price-box-footer .btn{height: 44px; font-size: 18px;}
.price-box-footer .btn-default{outline: none; text-shadow: none; background: none; border: 1px solid #eaeaea; color:#d2d2d2; box-shadow: none; font-size: 24px; padding-left: 0; padding-right: 0; line-height: 30px;}
.price-box-footer .btn-default:hover{outline: none; text-shadow: none; background: none; border: 1px solid #ff4d55; color:#d2d2d2; box-shadow: none; font-size: 24px;}
.we-accept{display: block; margin: 10px 0; text-align: center;}
.we-accept p{margin:0; text-align: center; font-size: 12px; color:#818181}
.we-accept p span{padding: 0 2px; display: inline-block;}

.month-table tr td{text-align: center; background: #6bbe86; border: 1px solid #fff!important; color:#fff}
.month-table tr td.red-bg{background: #ff4d55;}

.minimum-stay{padding: 0; list-style-type: none; margin-bottom: 20px}
.minimum-stay li{margin-bottom: 10px; position: relative; padding-left: 25px; font-size: 16px;}
.minimum-stay li span{position: absolute; left: 0; top:-2px}

.home-map{width:100%; height:500px; position: relative;}
.home-map .search-location{width:450px; height:40px; position: absolute; top:10px; left:10px; border: 1px solid #ccc; border-radius: 4px; background: #fff; padding: 5px; z-index: 1}
.home-map .search-location input[type="text"]{border: 0; width:85%}
.home-map .search-location span{display:  inline-block; padding: 0 3px; font-size:18px; color:#ababab }


.other-bedroom-section{width:100%; padding-bottom: 20px}
.other-bedroom-section h3{margin-bottom: 20px}
.properties-around-wrapper{overflow-x: scroll; overflow-y: hidden;}
.other-bedroom-section ul.prod-listing{float:none; margin: 0; white-space: nowrap; display: inline-block;}
.other-bedroom-section ul.prod-listing li{float:none; margin: 0; width:280px; margin-right: 10px; border: 4px solid transparent; display: inline-block; margin-bottom:10px}
.other-bedroom-section ul.prod-listing li .available{color:#909090; margin-top:5px; margin-bottom: 2px}
.other-bedroom-section ul.prod-listing li a.total-wrapper{display: block; text-decoration: none;}
.other-bedroom-section ul.prod-listing li:hover{border: 4px solid #ff4d55; border-bottom:2px solid #ff4d55; transition: ease all 0.2s}
.other-bedroom-section ul.prod-listing li .product-info{border-bottom:2px solid #ff4d55; float: left; width:100%}
.other-bedroom-section ul.prod-listing li .product-info .icon-holder{float: left;}


.description-holdr{padding:10px 25px 25px; background: #fff; overflow: hidden; margin-bottom: 30px}
.description-holdr p{font-size:16px; font-weight: 300}
.description-list ul{list-style-type: disc; list-style-position: inside; padding: 0;}
.description-list ul li{font-size:16px; font-weight: 300; margin-bottom: 5px}

.property-info-body::after, .property-info-body::before {content: " "; display: table;}
.property-info-body::after, .property-info-body::before {clear: both;}
/*.property-info-body{width:100%; float: left; border: 1px solid red}*/
.property-info-body .room-feature{color:#505050;float: left;font-size: 14px;margin: 0 20px 15px 0;}
.property-info-body .room-feature span{display: inline-block; line-height: 16px; vertical-align: top; margin-right: 8px}

.like-holder{padding:20px}
.like-holder .like-icon{height: 60px; width:60px; border-radius: 100%; display: block; margin: 0 auto; background: #ff4d55; text-align: center; color:#fff; font-size: 30px}
.like-holder .like-icon i{line-height: 60px;}
.like-holder ul{padding: 0; list-style-type: none;}
.like-holder ul li{padding: 0; position: relative; font-size:16px; font-weight: 300; margin-bottom: 6px; padding-left: 15px;}
.like-holder ul li::before{ position: absolute; font-size:8px; left:0; top:7px; font-family: 'FontAwesome'; content: '\f111'}







/************************* Signin ***********************/

.modal-content.signin > .modal-header {border-bottom: none;}
.modal-content.signin > .modal-footer {border-top: none;}
.modal-content.signin > .modal-body {width: 90%; margin: 0 auto; margin-top: -15px;}
.btn.btn-primary.face {width: 100%; margin:0 0 0px 0; background: #284786; color: #fff; text-align: left;}
