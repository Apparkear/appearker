    .city_holder .city_box .city_photo{height: auto;}
    .team_header button{font-size:16px}
    .footer_top ul li{margin: 0 2px;}
    .partner ul li{height:50px;}
    .partner ul{text-align: center;}
    .bannerBack{height: auto !important;padding-top: 100px !important;background-size:cover !important;}
    .banner{background-size:cover !important;background-repeat: no-repeat !important;}
    .videoTour ul li{width:33%;margin: 0}
}

@media(max-width:640px) {
	.layout-search{position: static;}
	.search-menu, .search-map, .l-main-menu{display: none;}
	.layout-search .search-sidebar {flex-basis: auto;padding-top: 76px;height: auto; width: 100%;}
	.layout-search .search-context {position: fixed;top: 0;width: 100%;z-index: 9;}
	.search-menu-icon{display: block !important;}
	.home-map .search-location{width: 90%}
	
	.l-main-section{padding-left: 0}
	.tab-pane ul li{height: 300px}
	
	.menu-mobile-disabled {background-color: #fff !important; height: 72px; width: auto; z-index: 999;}
	.l-property-body {padding: 2rem;}
}
@media(max-width:480px) {
	.prod-listing li {width: 96%;}
	.tab-pane ul li{height: 200px}
}
@media(max-width:360px) {
	.search-filter-header_results h4{font-size:16px}
	
}




















