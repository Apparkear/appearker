	.copy_right p,.copy_right span,.copy_right img{display: block;
    float: none !important;
    margin: 8px auto;
    text-align: center;}
    .lookingfor ul li,.lookingfor{width:100%;}
    .what_looking_back .button .btn-primary{width: 100%}
    
    
    /*.navbar.navbar-default{margin: 0;background:rgba(0,0,0,.5)}*/
