	.modal-dialog {width: 450px;}
	}	
@media(max-width:767px) {
	.searchback{background-size:cover !important;margin:15px 0 }
	.value_box{margin:15px 0}
	.member_box{margin:15px 0}
	.member_photo{height:150px}
	.clint_box{width:100%}
