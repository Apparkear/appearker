.btn-primary{color:#fff; background: #ff4d55; border: 0px; text-shadow:none; box-shadow: none; padding: 7px 14px}
.btn-primary:hover, .btn-primary:active, .btn-primary:active:hover, .btn-primary:focus{color:#fff; background: #e6333b; border: 0px; text-shadow:none; box-shadow: none; transition: ease all 0.2s;}

.layout-search {-moz-box-direction: normal;-moz-box-orient: horizontal;display: flex;flex-flow: row nowrap;overflow: hidden;position: fixed; width: 100%;}
.layout-search .search-menu {-moz-box-flex: 0;flex-grow: 0;position: relative;width: 130px;}
.layout-search .menu {background: #333332;bottom: 0;left: 0;position: absolute;right: 0;top: 0; padding: 15px 0;}
.menu__items-main-top {text-align: center;font-size: 12px;font-weight: 700;text-transform: uppercase;}


.layout-search .search-sidebar {max-width: 700px;-moz-box-direction: normal; -moz-box-flex: 1;-moz-box-orient: vertical;background-color: #e5e5e5;display: flex;flex-flow: column nowrap;flex-grow: 1;height: 100vh;overflow: hidden;position: relative; width: 56.5%;}
.layout-search .search-context {background-color: #fff;box-shadow: 0 -1px 0 #f7f5f5 inset;box-sizing: border-box;flex-shrink: 0;padding: 15px 20px;}
.pac-wrapper {-moz-box-direction: normal;-moz-box-orient: horizontal;display: flex;flex-flow: row nowrap;line-height: 46px;}
.search-autocomplete .pac-input-icon {height: 48px; font-size:20px}
.icon_grey{color:#959595}
.pac-input-icon {align-self: center;padding: 0;}
.search-autocomplete .pac-city, .search-autocomplete .pac-input {font-size: 16px;line-height: 20px; margin: 0;}
.pac-input {border: medium none;cursor: pointer;display: block;font-size: 0.9rem;line-height: 3.1em;overflow: hidden;padding: 0 8px;text-overflow: ellipsis; white-space: nowrap;width: 100%;}
.layout-search .search-scroll {background-color: #fff;overflow: auto;position: relative;}
.layout-search .search-simple-filters {box-sizing: border-box;padding: 0 20px;}
.layout-search .search-menu-icon {-moz-user-select: none;box-sizing: border-box;color: #a5a5a5;cursor: pointer;display: none;font-size: 24px;text-align: right;padding: 8px;position: absolute;right: 10px; top: 10px;}
.filter-group::after, .filter-group::before {content: " ";display: table;}
.filter-group::after {clear: both;}


.filter-group-datepicker .filter-group_label {margin-top: 22px;}
.filter-group_label {float: left;margin: 6px 0;width: 33%;}
.filter-group_element {float: right;width: 66%;}

