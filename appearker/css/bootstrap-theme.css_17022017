.city-selector__pikaday-container .pika-single, .form-datepicker_container {border: 1px solid #cbcbcb;border-radius: 4px;line-height: 3.52941rem;margin-bottom: 1.41176rem;padding: 0 ;position: relative;overflow: hidden;}
.form-datepicker_from{float: left;width: 50%; border: 0; padding-left: 12px; cursor: pointer;}
.form-datepicker_to{float: left;width: 50%; border: 0; padding-left: 12px; cursor: pointer;}
.filter-group:not(:first-child) {
    border-top: 1px solid #f7f5f5;
    padding-top: 24px;
}

.search-filter-header{padding:15px 20px; transition:opacity 0.3s ease-in 0s; border-top:1px solid #e5e5e5; border-bottom:3px solid #ff4d55 }
.search-filter-header_results {-moz-box-align: center;align-items: center;display: flex;}
.filter-header_results {-moz-box-pack: end;color: #32b496;display: flex;flex-shrink: 0;justify-content: flex-end;padding-left: 24px;text-align: right;}

.menu_items-main-top ul, .menu_items-secondary-bottom ul{list-style-type: none; padding: 0}
.menu_items-main-top ul li, .menu_items-secondary-bottom ul li{display: block;}
.menu_items-main-top ul li a{display: block; text-align: center; color:#fff; text-transform: uppercase; padding: 15px}
.menu_items-main-top ul li a:hover, .menu_items-secondary-bottom ul li a:hover{color:#ff4d55; text-decoration: none; background: #262625; transition: ease all 0.2s}
.menu_items-main-top ul li a i{display: block; text-align: center; font-size:38px; line-height: 44px;}
.menu_items-secondary-bottom {bottom: 0;font-size: 14px;font-weight: 400;left: 0; margin-bottom: 93px;position: absolute;right: 0;margin-bottom: 123px;text-align: center;}
.menu_items-secondary-bottom ul li a{display: block; text-align: center; color:#fff; padding: 15px}


.extra-filter{padding: 0 20px; display: none;}
.filter-group-row{border-top:1px solid #f7f5f5; padding-top: 15px; padding-bottom: 5px}
.checkbox-holder{display: block; position: relative; padding-left:26px; margin-bottom: 10px}
.checkbox-holder span{position: absolute; left:0; top:0; background-color: #fff;border: 1px solid #cccccc;border-radius: 3px;height: 18px;left: 0;margin-left: 0;position: absolute;width: 18px;}
.checkbox-holder input[type="checkbox"]{opacity: 0;z-index: 1; left: 3px;position: absolute;}
.checkbox-holder label{font-weight:normal; margin: 0}
.checkbox-holder input[type="checkbox"]:checked + span:before {background: #64a538; transition: ease all 0.2s; content: '\f00c'; font-family: FontAwesome; color:#fff; width:16px; height: 16px; font-size:11px; position: absolute; text-align: center;}

.form-group-table {float: left;margin: 12px 0; display: table;width: 100%;}
.form-group-table .simple-table {display: table-cell;}
.form-group-table .simple-table .simple-toggle_label {box-sizing: border-box; font-weight: normal;min-width: 30px;text-align: center;width: 100%; -moz-user-select: none;animation: 0.4s ease-out 0s normal none 1 running toggle-out-transparent;background: #fff none repeat scroll 0 0;border: 1px solid #cbcbcb;color: #737371;cursor: pointer;display: inline-block;line-height: 40px;padding: 0; cursor: pointer; margin: 0}
.simple-table:first-child .simple-toggle_label{border-top-left-radius: 4px; border-bottom-left-radius: 4px;}
.simple-table:last-child .simple-toggle_label{border-top-right-radius: 4px; border-bottom-right-radius: 4px;}

.product-list-holder{padding:10px 20px 0; overflow: hidden; background:#E5E5E5; overflow: hidden;}
.prod-listing{list-style-type: none; padding: 0; margin-left: -10px; margin-right: -10px; overflow: hidden;}
